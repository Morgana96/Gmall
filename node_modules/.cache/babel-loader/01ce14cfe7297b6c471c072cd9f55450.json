{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport throttle from \"lodash/throttle\";\nexport default {\n  name: \"ShopCart\",\n\n  data() {\n    return {\n      isSelectAll: true\n    };\n  },\n\n  mounted() {\n    this.getData();\n  },\n\n  methods: {\n    getData() {\n      this.$store.dispatch(\"getCartList\");\n    },\n\n    handler: throttle(async function (type, change, list) {\n      switch (type) {\n        case \"add\":\n          change = 1;\n          break;\n\n        case \"reduce\":\n          change = list.skuNum > 1 ? -1 : 0;\n          break;\n\n        case \"change\":\n          if (isNaN(change) || change < 1) {\n            change = 0;\n          } else {\n            change = parseInt(change) - list.skuNum;\n          }\n\n          break;\n      }\n\n      try {\n        await this.$store.dispatch(\"AddCart\", {\n          skuId: list.skuId,\n          skuNum: change\n        });\n        this.getData();\n      } catch (error) {}\n    }, 500),\n\n    async deletecart(item) {\n      try {\n        await this.$store.dispatch(\"deleteCart\", item.skuId);\n        this.getData();\n      } catch (error) {\n        alert(error.message);\n      }\n    },\n\n    async changechecked(item, event) {\n      try {\n        let isChecked = event.target.checked ? \"1\" : \"0\";\n        await this.$store.dispatch(\"changechecked\", {\n          skuId: item.skuId,\n          isChecked\n        });\n        this.getData();\n      } catch (error) {\n        alert(error.message);\n      }\n    },\n\n    deleteCheckedItems() {\n      const checkedItems = this.cartInfoList.filter(item => item.isChecked);\n      console.log(checkedItems);\n      this.$store.dispatch(\"deleteCart\", list.skuId);\n    },\n\n    selectAll() {\n      this.isSelectAll = !this.isSelectAll;\n      this.cartInfoList.forEach(element => {\n        element.isChecked = this.isSelectAll ? 1 : 0;\n      });\n    }\n\n  },\n  computed: { ...mapGetters([\"cartList\"]),\n\n    // singleprice(){\n    //   return this.list.skuNum * this.list.skuPrice\n    // },\n    cartInfoList() {\n      return this.cartList.cartInfoList || [];\n    },\n\n    totalPrice() {\n      let sum = 0;\n      this.cartInfoList.forEach(item => {\n        sum += item.skuNum * item.skuPrice;\n      });\n      return sum;\n    } // isChecked() {\n    //   return this.cartInfoList.every((item) => item.isChecked == 1);\n    // },\n\n\n  }\n};","map":{"version":3,"mappings":"AAwFA;AACA;AAEA;EACAA,gBADA;;EAEAC;IACA;MACAC;IADA;EAGA,CANA;;EAOAC;IACA;EACA,CATA;;EAUAC;IACAC;MACA;IACA,CAHA;;IAKAC;MACA;QACA;UACAC;UACA;;QAEA;UACAA;UACA;;QAEA;UACA;YACAA;UACA,CAFA,MAEA;YACAA;UACA;;UACA;MAfA;;MAiBA;QACA;UACAC,iBADA;UAEAC;QAFA;QAIA;MACA,CANA,CAMA;IACA,CAzBA,EAyBA,GAzBA,CALA;;IAgCA;MACA;QACA;QACA;MACA,CAHA,CAGA;QACAC;MACA;IACA,CAvCA;;IAyCA;MACA;QACA;QACA;UAAAF;UAAAG;QAAA;QACA;MACA,CAJA,CAIA;QACAD;MACA;IACA,CAjDA;;IAmDAE;MACA;MACAC;MACA;IAEA,CAxDA;;IAyDAC;MACA;MAEA;QACAC;MACA,CAFA;IAGA;;EA/DA,CAVA;EA4EAC,YACA,2BADA;;IAEA;IACA;IACA;IACAC;MACA;IACA,CAPA;;IAQAC;MACA;MACA;QACAC;MACA,CAFA;MAGA;IACA,CAdA,CAgBA;IACA;IACA;;;EAlBA;AA5EA","names":["name","data","isSelectAll","mounted","methods","getData","handler","change","skuId","skuNum","alert","isChecked","deleteCheckedItems","console","selectAll","element","computed","cartInfoList","totalPrice","sum"],"sourceRoot":"src/pages/ShopCart","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <h4>全部商品</h4>\r\n    <div class=\"cart-main\">\r\n      <div class=\"cart-th\">\r\n        <div class=\"cart-th1\">全部</div>\r\n        <div class=\"cart-th2\">商品</div>\r\n        <div class=\"cart-th3\">单价（元）</div>\r\n        <div class=\"cart-th4\">数量</div>\r\n        <div class=\"cart-th5\">小计（元）</div>\r\n        <div class=\"cart-th6\">操作</div>\r\n      </div>\r\n      <div class=\"cart-body\">\r\n        <ul class=\"cart-list\" v-for=\"list in cartInfoList\" :key=\"list.id\">\r\n          <li class=\"cart-list-con1\">\r\n            <input\r\n              type=\"checkbox\"\r\n              name=\"chk_list\"\r\n              :checked=\"list.isChecked == 1\"\r\n              @change=\"changechecked(list, $event)\"\r\n            />\r\n          </li>\r\n          <li class=\"cart-list-con2\">\r\n            <img :src=\"list.imgUrl\" />\r\n            <div class=\"item-msg\">{{ list.skuName }}</div>\r\n          </li>\r\n\r\n          <li class=\"cart-list-con4\">\r\n            <span class=\"price\">{{ list.skuPrice }}</span>\r\n          </li>\r\n          <li class=\"cart-list-con5\">\r\n            <a\r\n              href=\"javascript:void(0)\"\r\n              class=\"mins\"\r\n              @click=\"handler('reduce', -1, list)\"\r\n              >-</a\r\n            >\r\n            <input\r\n              autocomplete=\"off\"\r\n              type=\"text\"\r\n              :value=\"list.skuNum\"\r\n              @change=\"handler('change', $event.target.value * 1, list)\"\r\n              minnum=\"1\"\r\n              class=\"itxt\"\r\n            />\r\n            <a\r\n              href=\"javascript:void(0)\"\r\n              class=\"plus\"\r\n              @click=\"handler('add', 1, list)\"\r\n              >+</a\r\n            >\r\n          </li>\r\n          <li class=\"cart-list-con6\">\r\n            <span class=\"sum\">{{ list.skuNum * list.skuPrice }}</span>\r\n          </li>\r\n          <li class=\"cart-list-con7\">\r\n            <a class=\"sindelet\" @click=\"deletecart(list)\">删除</a>\r\n            <br />\r\n            <a href=\"#none\">移到收藏</a>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <div class=\"cart-tool\">\r\n      <div class=\"select-all\">\r\n        <input class=\"chooseAll\" type=\"checkbox\" @change=\"selectAll\" :checked=\"isSelectAll\"/>\r\n        <span>全选</span>\r\n      </div>\r\n      <div class=\"option\">\r\n        <a @click=\"deleteCheckedItems\">删除选中的商品</a>\r\n        <a href=\"#none\">移到我的关注</a>\r\n        <a href=\"#none\">清除下柜商品</a>\r\n      </div>\r\n      <div class=\"money-box\">\r\n        <div class=\"chosed\">已选择 <span>0</span>件商品</div>\r\n        <div class=\"sumprice\">\r\n          <em>总价（不含运费） ：</em>\r\n          <i class=\"summoney\">{{ totalPrice }}</i>\r\n        </div>\r\n        <div class=\"sumbtn\">\r\n          <a class=\"sum-btn\" href=\"###\" target=\"_blank\">结算</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport throttle from \"lodash/throttle\";\r\n\r\nexport default {\r\n  name: \"ShopCart\",\r\n  data(){\r\n    return {\r\n      isSelectAll : true\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n  },\r\n  methods: {\r\n    getData() {\r\n      this.$store.dispatch(\"getCartList\");\r\n    },\r\n\r\n    handler: throttle(async function (type, change, list) {\r\n      switch (type) {\r\n        case \"add\":\r\n          change = 1;\r\n          break;\r\n\r\n        case \"reduce\":\r\n          change = list.skuNum > 1 ? -1 : 0;\r\n          break;\r\n\r\n        case \"change\":\r\n          if (isNaN(change) || change < 1) {\r\n            change = 0;\r\n          } else {\r\n            change = parseInt(change) - list.skuNum;\r\n          }\r\n          break;\r\n      }\r\n      try {\r\n        await this.$store.dispatch(\"AddCart\", {\r\n          skuId: list.skuId,\r\n          skuNum: change,\r\n        });\r\n        this.getData();\r\n      } catch (error) {}\r\n    }, 500),\r\n\r\n    async deletecart(item) {\r\n      try {\r\n        await this.$store.dispatch(\"deleteCart\", item.skuId);\r\n        this.getData();\r\n      } catch (error) {\r\n        alert(error.message);\r\n      }\r\n    },\r\n\r\n    async changechecked(item, event) {\r\n      try {\r\n        let isChecked = event.target.checked ? \"1\" : \"0\";\r\n        await this.$store.dispatch(\"changechecked\", { skuId: item.skuId, isChecked });\r\n        this.getData()\r\n      } catch (error) {\r\n        alert(error.message)\r\n      }\r\n    },\r\n\r\n    deleteCheckedItems(){\r\n      const checkedItems = this.cartInfoList.filter(item =>item.isChecked)\r\n      console.log(checkedItems);\r\n      this.$store.dispatch(\"deleteCart\", list.skuId)\r\n\r\n    },\r\n    selectAll(){\r\n      this.isSelectAll = !this.isSelectAll\r\n\r\n      this.cartInfoList.forEach(element => {\r\n        element.isChecked = this.isSelectAll ? 1 : 0\r\n      });\r\n    }\r\n\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"cartList\"]),\r\n    // singleprice(){\r\n    //   return this.list.skuNum * this.list.skuPrice\r\n    // },\r\n    cartInfoList() {\r\n      return this.cartList.cartInfoList || [];\r\n    },\r\n    totalPrice() {\r\n      let sum = 0;\r\n      this.cartInfoList.forEach((item) => {\r\n        sum += item.skuNum * item.skuPrice;\r\n      });\r\n      return sum;\r\n    },\r\n\r\n    // isChecked() {\r\n    //   return this.cartInfoList.every((item) => item.isChecked == 1);\r\n    // },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.cart {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n\r\n  h4 {\r\n    margin: 9px 0;\r\n    font-size: 14px;\r\n    line-height: 21px;\r\n  }\r\n\r\n  .cart-main {\r\n    .cart-th {\r\n      background: #f5f5f5;\r\n      border: 1px solid #ddd;\r\n      padding: 10px;\r\n      overflow: hidden;\r\n\r\n      & > div {\r\n        float: left;\r\n      }\r\n\r\n      .cart-th1 {\r\n        width: 25%;\r\n\r\n        input {\r\n          vertical-align: middle;\r\n        }\r\n\r\n        span {\r\n          vertical-align: middle;\r\n        }\r\n      }\r\n\r\n      .cart-th2 {\r\n        width: 25%;\r\n      }\r\n\r\n      .cart-th3,\r\n      .cart-th4,\r\n      .cart-th5,\r\n      .cart-th6 {\r\n        width: 12.5%;\r\n      }\r\n    }\r\n\r\n    .cart-body {\r\n      margin: 15px 0;\r\n      border: 1px solid #ddd;\r\n\r\n      .cart-list {\r\n        padding: 10px;\r\n        border-bottom: 1px solid #ddd;\r\n        overflow: hidden;\r\n\r\n        & > li {\r\n          float: left;\r\n        }\r\n\r\n        .cart-list-con1 {\r\n          width: 15%;\r\n        }\r\n\r\n        .cart-list-con2 {\r\n          width: 35%;\r\n\r\n          img {\r\n            width: 82px;\r\n            height: 82px;\r\n            float: left;\r\n          }\r\n\r\n          .item-msg {\r\n            float: left;\r\n            width: 150px;\r\n            margin: 0 10px;\r\n            line-height: 18px;\r\n          }\r\n        }\r\n\r\n        .item-txt {\r\n          text-align: center;\r\n        }\r\n      }\r\n\r\n      .cart-list-con4 {\r\n        width: 10%;\r\n      }\r\n\r\n      .cart-list-con5 {\r\n        width: 17%;\r\n\r\n        .mins {\r\n          border: 1px solid #ddd;\r\n          border-right: 0;\r\n          float: left;\r\n          color: #666;\r\n          width: 6px;\r\n          text-align: center;\r\n          padding: 8px;\r\n        }\r\n\r\n        input {\r\n          border: 1px solid #ddd;\r\n          width: 40px;\r\n          height: 33px;\r\n          float: left;\r\n          text-align: center;\r\n          font-size: 14px;\r\n        }\r\n\r\n        .plus {\r\n          border: 1px solid #ddd;\r\n          border-left: 0;\r\n          float: left;\r\n          color: #666;\r\n          width: 6px;\r\n          text-align: center;\r\n          padding: 8px;\r\n        }\r\n      }\r\n\r\n      .cart-list-con6 {\r\n        width: 10%;\r\n\r\n        .sum {\r\n          font-size: 16px;\r\n        }\r\n      }\r\n\r\n      .cart-list-con7 {\r\n        width: 13%;\r\n\r\n        a {\r\n          color: #666;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.cart-tool {\r\n  overflow: hidden;\r\n  border: 1px solid #ddd;\r\n\r\n  .select-all {\r\n    padding: 10px;\r\n    overflow: hidden;\r\n    float: left;\r\n\r\n    span {\r\n      vertical-align: middle;\r\n    }\r\n\r\n    input {\r\n      vertical-align: middle;\r\n    }\r\n  }\r\n\r\n  .option {\r\n    padding: 10px;\r\n    overflow: hidden;\r\n    float: left;\r\n\r\n    a {\r\n      float: left;\r\n      padding: 0 10px;\r\n      color: #666;\r\n    }\r\n  }\r\n\r\n  .money-box {\r\n    float: right;\r\n\r\n    .chosed {\r\n      line-height: 26px;\r\n      float: left;\r\n      padding: 0 10px;\r\n    }\r\n\r\n    .sumprice {\r\n      width: 200px;\r\n      line-height: 22px;\r\n      float: left;\r\n      padding: 0 10px;\r\n\r\n      .summoney {\r\n        color: #c81623;\r\n        font-size: 16px;\r\n      }\r\n    }\r\n\r\n    .sumbtn {\r\n      float: right;\r\n\r\n      a {\r\n        display: block;\r\n        position: relative;\r\n        width: 96px;\r\n        height: 52px;\r\n        line-height: 52px;\r\n        color: #fff;\r\n        text-align: center;\r\n        font-size: 18px;\r\n        font-family: \"Microsoft YaHei\";\r\n        background: #e1251b;\r\n        overflow: hidden;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}
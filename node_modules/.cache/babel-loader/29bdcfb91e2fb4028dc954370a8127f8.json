{"ast":null,"code":"export default {\n  name: \"Pagination\",\n  props: ['pageNo', 'pageSize', 'total', 'continues'],\n  computed: {\n    totalPage() {\n      return Math.ceil(this.total / this.pageSize);\n    },\n\n    startNum() {\n      let start = 0,\n          end = 0;\n\n      if (this.continues > this.totalPage) {\n        start = 1;\n        end = this.totalPage;\n      } else {\n        start = this.pageNo - Math.floor(this.continues / 2);\n        end = this.pageNo + Math.floor(this.continues / 2);\n\n        if (start <= 0) {\n          start = 1, end = this.continues;\n        }\n\n        if (end > this.totalPage) {\n          start = this.totalPage - this.continues + 1;\n          end = this.totalPage;\n        }\n      }\n\n      return {\n        start,\n        end\n      };\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkBA;EACAA,kBADA;EAEAC,mDAFA;EAGAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;MAAA;;MACA;QACAC;QACAC;MACA,CAHA,MAGA;QACAD;QACAC;;QACA;UACAD,WACAC,oBADA;QAEA;;QACA;UACAD;UACAC;QACA;MACA;;MACA;QAAAD;QAAAC;MAAA;IACA;;EAtBA;AAHA","names":["name","props","computed","totalPage","startNum","start","end"],"sourceRoot":"src/components/pagination","sources":["index.vue"],"sourcesContent":["<template>\r\n    <div class=\"pagination\">\r\n        \r\n        <button :disabled=\"pageNo==1\" @click=\"$emit('getpageNo',pageNo-1)\">上一页</button>\r\n        <button v-if=\"startNum.start\" @click=\"$emit('getpageNo',1)\" >1</button>\r\n        <button v-if=\"startNum.start >2\">···</button>\r\n\r\n        <button v-for=\"(page,index) in startNum.end\" :key=\"index\" v-if=\"page >= startNum.start\"  @click=\"$emit('getpageNo',page)\">{{page}}</button>\r\n\r\n        <button v-if=\"startNum.end <= totalPage-1\">···</button>\r\n        <button v-if=\"startNum.end <= totalPage\" @click=\"$emit('getpageNo',totalPage)\">{{totalPage}}</button>\r\n        <button :disabled=\"pageNo == totalPage\" @click=\"$emit('getpageNo',pageNo+1)\" >下一页</button>\r\n\r\n        <button style=\"margin-left: 30px\">共 {{total}} 条</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Pagination\",\r\n        props:['pageNo','pageSize','total','continues'],\r\n        computed:{\r\n            totalPage(){\r\n                return Math.ceil(this.total/this.pageSize)\r\n            },\r\n            startNum(){\r\n                 let start = 0, end = 0;\r\n                 if(this.continues>this.totalPage){\r\n                    start = 1;\r\n                    end = this.totalPage\r\n                 }else{\r\n                    start = this.pageNo - Math.floor(this.continues/2);\r\n                    end = this.pageNo + Math.floor(this.continues/2);\r\n                    if(start<=0){\r\n                        start = 1,\r\n                        end = this.continues\r\n                    }\r\n                    if(end > this.totalPage){\r\n                        start = this.totalPage - this.continues + 1\r\n                        end = this.totalPage\r\n                    }\r\n                 }\r\n                return { start,end }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .pagination {\r\n        text-align: center;\r\n        button {\r\n            margin: 0 5px;\r\n            background-color: #f4f4f5;\r\n            color: #606266;\r\n            outline: none;\r\n            border-radius: 2px;\r\n            padding: 0 4px;\r\n            vertical-align: top;\r\n            display: inline-block;\r\n            font-size: 13px;\r\n            min-width: 35.5px;\r\n            height: 28px;\r\n            line-height: 28px;\r\n            cursor: pointer;\r\n            box-sizing: border-box;\r\n            text-align: center;\r\n            border: 0;\r\n\r\n            &[disabled] {\r\n                color: #c0c4cc;\r\n                cursor: not-allowed;\r\n            }\r\n\r\n            &.active {\r\n                cursor: not-allowed;\r\n                background-color: #409eff;\r\n                color: #fff;\r\n            }\r\n        }\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module"}